{
  "name": "azsample",
  "version": "1.0.1",
  "description": "This package for samle data username phone etc.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "keywords": [
    "sample",
    "sampleaz",
    "randomdata",
    "sampledata"
  ],
  "author": "Nurlan Huseynov",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NurlanH/azsample.git"
  },
  "bugs": {
    "url": "https://github.com/NurlanH/azsample/issues"
  },
  "files": [
    "lib/**/*"
  ]
}
